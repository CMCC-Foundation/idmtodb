use idmdb;

DELIMITER ;; 
DROP PROCEDURE IF EXISTS insert_group;
CREATE PROCEDURE insert_group
(  
     IN p_name VARCHAR(32),
     IN p_desc VARCHAR(512),
     IN p_status TINYINT,
     IN p_gid INT,
     IN p_creation_date DATE
)  
   BEGIN
	INSERT INTO idmdb.group (name, idmdb.group.desc, status, gid, creation_date) VALUES (p_name, p_desc, p_status, p_gid, p_creation_date);
   END
   ;;
